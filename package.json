{
    "name": "@strateworks/sdk",
    "version": "1.0.0",
    "license": "UNLICENSED",
    "author": "Ian Torres",
    "type": "commonjs",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf dist",
        "test": "vitest --run",
        "test:watch": "vitest",
        "test:coverage": "vitest --run --coverage --reporter verbose",
        "minify": "find dist -name \"*.js\" -type f -exec terser {} --compress --mangle --output {} \\;",
        "format": "prettier --write ."
    },
    "dependencies": {
        "isomorphic-ws": "^5.0.0",
        "uuid": "^13.0.0",
        "ws": "^8.19.0"
    },
    "devDependencies": {
        "@types/node": "^22.15.2",
        "@vitest/coverage-v8": "3.1.3",
        "c8": "^10.1.3",
        "prettier": "^3.5.3",
        "terser": "^5.44.1",
        "typescript": "^5.8.3",
        "vite": "^6.3.4",
        "vitest": "^3.1.2"
    }
}
